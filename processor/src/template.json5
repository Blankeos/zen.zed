{
  $schema: "https://zed.dev/schema/themes/v0.2.0.json",
  name: "{{ name }}",
  author: "blankeos",
  themes: [
    {
      name: "{{ name }}",
      appearance: "{{ appearance }}",
      style: {
        "background.appearance": "{{ background.appearance }}",
        "border.variant": "#ffffff10",
        "border.focused": "#ffffff10",
        "border.selected": "#ffffff10",
        "border.transparent": "#ffffff10",
        "border.disabled": "#ffffff10",
        "elevated_surface.background": "{{ colors.background_static }}", // Tooltips, the card items in extensions
        "surface.background": "#19191a", // Codeblocks in README, scrollbar in extensions
        background: "{{ colors.background }}",
        "element.background": "#30334000", // Buttons in extensions
        "element.hover": "#30334080", // Panel(filetree) item hover
        "element.active": null, // Panel(filetree) click
        "element.selected": "#30334080", // Panel(filetree) focus
        "element.disabled": null,
        "drop_target.background": "#506477",
        "ghost_element.background": null,
        "ghost_element.hover": "#eff6ff0a",
        "ghost_element.active": null,
        "ghost_element.selected": "#eff6ff0a",
        "ghost_element.disabled": null,
        text: "#a6accd",
        "text.muted": "#767c9d",
        "text.placeholder": null,
        "text.disabled": null,
        "text.accent": "{{ colors.primary }}",
        icon: null,
        "icon.muted": null,
        "icon.disabled": null,
        "icon.placeholder": null,
        "icon.accent": null,
        "status_bar.background": "{{ colors.background }}",
        "title_bar.background": "{{ colors.background }}",
        //"title_bar.inactive_background": "#ecddb4ff",
        "toolbar.background": "#00000000", // The thing between tabs and buffer

        "tab_bar.background": "#00000000",
        "tab.inactive_background": "{{ colors.tab_bar.background }}",
        "tab.active_background": "{{ colors.tab.active_background }}",

        "search.match_background": "#dbeafe3d",
        "panel.background": "#12121200",
        "panel.overlay_background": "{{ colors.overlay_background }}", // Used for sticky background in the filetree
        "panel.overlay_hover": "{{ colors.overlay_background_hover }}",
        "panel.focused_border": null,
        "pane.focused_border": null,
        "scrollbar.thumb.background": "#00000080",
        "scrollbar.thumb.hover_background": "#a6accd25",
        "scrollbar.thumb.border": "#00000080",
        "scrollbar.track.background": "#12121200",
        "scrollbar.track.border": "#00000000",
        "editor.foreground": "#a6accd", // UI Text elements, text in autocomplete items + inside of html tags (in syntax weirdly), AI Assistant thread titles
        "editor.background": "{{ colors.editor.background }}", // The buffer area
        "editor.gutter.background": "{{ colors.editor.background }}",
        "editor.subheader.background": "{{ colors.background_static }}", // The dropdown target in Project Search
        "editor.active_line.background": "#b4b4b61a",
        "editor.highlighted_line.background": null,
        "editor.line_number": "#767c9dff",
        "editor.active_line_number": "{{ colors.primary }}",
        "editor.invisible": null,
        "editor.wrap_guide": "#00000030",
        "editor.active_wrap_guide": "#00000030",
        "editor.document_highlight.read_background": "#a855f733",
        "editor.document_highlight.bracket_background": "#54b3eb1d",
        "editor.document_highlight.write_background": null,
        "terminal.background": "{{ colors.terminal.background }}",
        "terminal.foreground": "#a6accd",
        "terminal.bright_foreground": null,
        "terminal.dim_foreground": null,
        "terminal.ansi.black": "{{ colors.background_static }}",
        "terminal.ansi.bright_black": "#73675e", // Predictive for the terminal (i.e. zsh-autocomplete)
        "terminal.ansi.dim_black": null,
        "terminal.ansi.red": "#d0679d",
        "terminal.ansi.bright_red": "#d0679d",
        "terminal.ansi.dim_red": null,
        "terminal.ansi.green": "#5DE4c7",
        "terminal.ansi.bright_green": "#5DE4c7",
        "terminal.ansi.dim_green": null,
        "terminal.ansi.yellow": "#fffac2",
        "terminal.ansi.bright_yellow": "#fffac2",
        "terminal.ansi.dim_yellow": null,
        "terminal.ansi.blue": "#89ddff",
        "terminal.ansi.bright_blue": "#ADD7FF",
        "terminal.ansi.dim_blue": null,
        "terminal.ansi.magenta": "#f087bd",
        "terminal.ansi.bright_magenta": "#f087bd",
        "terminal.ansi.dim_magenta": null,
        "terminal.ansi.cyan": "#89ddff",
        "terminal.ansi.bright_cyan": "#ADD7FF",
        "terminal.ansi.dim_cyan": null,
        "terminal.ansi.white": "#ffffff",
        "terminal.ansi.bright_white": "#ffffff",
        "terminal.ansi.dim_white": null,

        "link_text.hover": "#ADD7FF",

        // Git Diffs
        "version_control.added": "#5DE4c7",
        "version_control.deleted": "#ff0000",
        "version_control.modified": "#eed49f",
        "version_control.renamed": "#7dc4e4",
        "version_control.conflict": "#f5a97f",
        "version_control.ignored": "#6e738d",

        conflict: "#d0679d",
        "conflict.background": "#4a2a33",
        "conflict.border": "#ffffff10",

        created: "#5fb3a1",
        "created.background": "#294113ff",
        "created.border": "#ffffff10",

        deleted: "#d0679d",
        "deleted.background": "#48161bff",
        "deleted.border": "#ffffff10",

        error: "#d0679d",
        "error.background": "#590a0f",
        "error.border": "#771617",

        hidden: "#767c9d",
        "hidden.background": "{{ colors.background_static }}",
        "hidden.border": "#ffffff10",

        hint: "#969696ff",
        "hint.background": "{{ colors.background_static }}",
        "hint.border": "#ffffff10",

        ignored: "#606586",
        "ignored.background": "{{ colors.background_static }}",
        "ignored.border": "#ffffff10",

        // Used for Inline AI Assistant
        info: "#ADD7FF",
        "info.background": "#43525f",
        "info.border": "#60a5fa55",

        modified: "#ADD7FF",
        "modified.background": "#121212",
        "modified.border": "#ffffff10",

        // Color of AI Tab Autocomplete
        predictive: "#515671",
        "predictive.background": "#121212",
        "predictive.border": "#ffffff10",

        renamed: null,
        "renamed.background": "{{ colors.background_static }}",
        "renamed.border": "#ffffff10",

        success: null,
        "success.background": "{{ colors.background_static }}",
        "success.border": "#ffffff10",

        unreachable: null,
        "unreachable.background": "{{ colors.background_static }}",
        "unreachable.border": "#ffffff10",

        warning: "#fffac2",
        "warning.background": "{{ colors.background_static }}",
        "warning.border": "#ffffff10",

        // Cursor colors, first item is you, others are the other items
        players: [
          {
            cursor: "#bae6fd",
            selection: "#60a5fa66",
          },
        ],

        syntax: {
          attribute: {
            color: "#91b4d5",
            font_style: "italic",
            font_weight: null,
          },
          boolean: {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          comment: {
            color: "#767c9dB0",
            font_style: "italic",
            font_weight: null,
          },
          "comment.doc": {
            color: "#767c9dB0",
            font_style: "italic",
            font_weight: null,
          },
          constant: {
            color: "#e4f0fb",
            font_style: null,
            font_weight: null,
          },
          "constant.builtin": {
            color: "#d0679d",
            font_style: null,
            font_weight: null,
          },
          constructor: {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          emphasis: {
            color: "#7390AA",
            font_style: "italic",
            font_weight: null,
          },
          "emphasis.strong": {
            color: "#7390AA",
            font_style: null,
            font_weight: 700,
          },
          keyword: {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          label: {
            color: "#91B4D5",
            font_style: null,
            font_weight: null,
          },
          link_text: {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          link_uri: {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          number: {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          operator: {
            color: "#91B4D5",
            font_style: null,
            font_weight: null,
          },
          punctuation: {
            color: "#a6accd",
            font_style: null,
            font_weight: null,
          },
          "punctuation.bracket": {
            color: "#a6accd",
            font_style: null,
            font_weight: null,
          },
          "punctuation.delimiter": {
            color: "#a6accd",
            font_style: null,
            font_weight: null,
          },
          "punctuation.list_marker": {
            color: "#a6accd",
            font_style: null,
            font_weight: null,
          },
          "punctuation.special": {
            color: "#a6accd",
            font_style: null,
            font_weight: null,
          },
          string: {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          "string.escape": {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          "string.regex": {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          "string.special": {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          "string.special.symbol": {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          tag: {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          "text.literal": {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          title: {
            color: "#91B4D5",
            font_style: null,
            font_weight: null,
          },
          function: {
            color: "#add7ff",
            font_style: null,
            font_weight: null,
          },
          namespace: {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          module: {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          property: {
            color: "#ffffff",
            font_style: null,
            font_weight: null,
          },
          type: {
            color: "#a6accdC0",
            font_style: null,
            font_weight: null,
          },
          // Works for string | number | etc
          // "type.builtin": {
          //   "color": "{{ colors.primary }}",
          //   "font_style": null,
          //   "font_weight": null
          // },
          "type.interface": {
            color: "{{ colors.primary }}",
            font_style: null,
            font_weight: null,
          },
          variable: {
            color: "#e4f0fb",
            font_style: "italic",
            font_weight: null,
          },
          "variable.special": {
            color: "{{ colors.primary }}",
            font_style: "italic",
            font_weight: null,
          },
          // "tag.jsx": {
          //   "color": "{{ colors.primary }}",
          //   "font_style": null,
          //   "font_weight": null
          // },
          "punctuation.bracket.jsx": {
            color: "#ffffff",
            font_style: null,
            font_weight: null,
          },
          // "attribute.jsx": {
          //   "color": "#ff0000",
          //   "font_style": null,
          //   "font_weight": null
          // },
          // "punctuation.delimiter.jsx": {
          //   "color": "#ff0000",
          //   "font_style": null,
          //   "font_weight": null
          // },
          // "embedded": {
          //   "color": "#ff0000",
          //   "font_style": null,
          //   "font_weight": null
          // }
        },
      },
    },
  ],
}
